/* pages/discovery/discovery.wxss */
.container {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -ms-flex-direction: column;
  flex-direction: column;
}
.nav-boxes-outer {
  background-color: white;
  height: 160rpx;
  width: 100vw;
   display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -ms-flex-direction: row;
  flex-direction: row;
}
.nav-box-wrap {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1; 
  text-align: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  justify-content: center;
  align-items: center;
  -ms-flex-direction: column;
  flex-direction: column;
}
.nav-box {
  height: 100rpx;
  width: 100%;
  border-left: 1px solid #f6f6f6;
}
.nav-box-wrap:first-child .nav-box{
  border-left: none;
}
.nav-img-wrap{
  width: 100%;
  height: 60rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  justify-content: center;
  align-items: center;
}
.nav-img {
  width: 60rpx;
  height: 60rpx;
}
.nav-title {
  color: #808080;
  font-size: 28rpx;
}
.cellList-boxes-outer {
  background-color: #ebebeb;
}
