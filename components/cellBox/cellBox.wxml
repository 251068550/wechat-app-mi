<!--components/cellBox/cellBox.wxml-->
<view class='cell-box-outer' hover-class="cell-enter">
  <view class='cell-box-inner'>

    <view class='cell-img-wrap'>
      <image lazy-load='{{ true }}' class='cell-img' src='{{ innerData.imgSrc }}'></image>
    </view>
    <text class='cell-title'>{{ innerData.title }}</text>
    
  </view>
</view>
